{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "semi": 2,
        "no-console": 0
    },
    "plugins": [],
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "describe": false,
        "it": false
    }
}
