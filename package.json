  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.1.1",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "should": "^10.0.0"
  }
